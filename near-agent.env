# NEAR AI Deepfake Detection Agent Configuration
# ==============================================

# NEAR Network Configuration
NEAR_NETWORK=testnet
NEAR_NODE_URL=https://rpc.testnet.near.org
NEAR_WALLET_URL=https://wallet.testnet.near.org
NEAR_HELPER_URL=https://helper.testnet.near.org
NEAR_EXPLORER_URL=https://explorer.testnet.near.org

# Agent Configuration
AGENT_NAME=deepfake-detector
AGENT_VERSION=1.0.0
AGENT_ACCOUNT=deepfake-agent.testnet
AGENT_DESCRIPTION="NEAR Shade AI Agent for autonomous deepfake detection and verification"

# Smart Contract Configuration
CONTRACT_ACCOUNT=deepfake-contract.testnet
CONTRACT_NAME=deepfake-detection
CONTRACT_VERSION=1.0.0

# AI Model Configuration
MODEL_NAME=llama-v3p1-70b-instruct
MODEL_PROVIDER=fireworks
MODEL_TEMPERATURE=0.3
MODEL_MAX_TOKENS=4096

# Agent Capabilities
SUPPORTED_FORMATS=jpg,jpeg,png,mp4,avi,mov,webm
MAX_FILE_SIZE=50MB
DETECTION_TIMEOUT=30s
CONFIDENCE_THRESHOLD=60

# 0G Network Configuration (Mock for now)
ZEROG_NETWORK=testnet
ZEROG_ENDPOINT=https://testnet.0g.ai
ZEROG_STORAGE_ENABLED=false

# Application URLs
NEARAI_APP_URL=https://app.near.ai/agents/deepfake-agent.testnet/deepfake-detector
NEARAI_REGISTRY_URL=https://registry.near.ai

# Project Information
PROJECT_NAME="NEAR Shade Agent + 0G Network MVP"
PROJECT_VERSION=1.0.0
PROJECT_DESCRIPTION="1-Day Hackathon MVP for decentralized deepfake detection"
HACKATHON_NAME="One Trillion Agents Hackathon"
PRIZE_POOL="$102,550"

# Development Configuration
DEV_MODE=true
DEBUG_LOGGING=true
MOCK_DETECTION=true
DEMO_MODE=true

# Gas Configuration
DEFAULT_GAS=50000000000000
DEPLOY_GAS=100000000000000
CONTRACT_CALL_GAS=30000000000000

# File Paths
AGENT_PATH=./deepfake-agent.testnet/deepfake-detector
CONTRACT_PATH=./deepfake-detector-contract
FRONTEND_PATH=./frontend

# Security & Privacy
TEE_ENABLED=false
ATTESTATION_REQUIRED=false
PRIVACY_MODE=true

# Analytics & Monitoring
TRACK_USAGE=true
STORE_RESULTS=true
BLOCKCHAIN_LOGGING=true

# Demo Configuration
DEMO_FILES_PATH=./demo-files
SAMPLE_DEEPFAKE_RATE=33
CONFIDENCE_RANGE_MIN=60
